{
  "name": "@jiphyeonjeon-42/backend",
  "version": "1.0.0",
  "description": "3rd of 42-jiphyeonjeon web service backend",
  "main": "./dist/server.js",
  "repository": "https://github.com/jiphyeonjeon-42/backend-express.git",
