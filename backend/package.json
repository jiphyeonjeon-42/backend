    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "kysely-codegen": "^0.10.1",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typeorm-model-generator": "^0.4.6"
